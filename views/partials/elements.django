<div class="card card-compact w-96 bg-primary shadow-xl" role="article">
    <div class="card-body">
        <h2 class="card-title">{{ planet.Name }}</h2>
        <p aria-label="Description">{{ planet.Desc | default:"Card description" }}</p>
        <div class="card-actions justify-end">
            <button class="btn btn-primary" aria-label="View details for {{ planet.Desc }}">
                View
            </button>
        </div>
    </div>  
</div>